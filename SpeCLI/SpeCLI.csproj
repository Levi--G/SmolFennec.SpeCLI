    <LangVersion>8.0</LangVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageId>SmolFennec.SpeCLI</PackageId>
    <Authors>Levi Gillis</Authors>
    <Product>SmolFennec.SpeCLI</Product>
